    "kolorist": "^1.8.0",
    "launch-editor-middleware": "^2.9.1",
    "lodash": "^4.17.21",
    "log-update": "^6.1.0",
    "mini-css-extract-plugin": "^2.9.1",
    "minimist": "^1.2.8",
